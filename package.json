{
  "author": "Tobias Kopelke <nox@raynode.de>",
  "dependencies": {
    "debug": "^2.6.8"
  },
  "peerDependencies": {
    "@raynode/nx-logger": "0.0.0"
  },
  "description": "",
  "devDependencies": {
    "@types/debug": "0.0.29",
