    "test-only": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "types": "dist/log.d.ts",
  "version": "0.0.0-development",
  "release": {
    "verifyRelease": {
      "path": "cracks",
      "paths": [
        "__tests__",
        "package.json"
      ]
    }
  }
